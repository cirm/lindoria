    "immutable": "^3.8.1",
    "material-ui": "0.15.2",
    "react": "^15.2.0",
    "react-addons-shallow-compare": "^15.2.0",
    "react-css-modules": "^3.7.7",
    "react-dom": "^15.2.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.0",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.5.1",
    "redux-form": "^6.0.0-rc.3",
